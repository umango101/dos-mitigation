;
; BIND reverse data file for local loopback interface
;
$TTL   604800
@      IN      SOA      ns.google.com. root.google.com (
                              2         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@      IN      NS       ns.
3      IN      PTR      ns.google.com.
155    IN      PTR      www.google.com.
